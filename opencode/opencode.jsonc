{
  "plugin": [
    "oh-my-opencode@latest",
    "opencode-antigravity-auth@1.4.6",
    "opencode-gemini-auth@latest"
  ],
  "$schema": "https://opencode.ai/config.json",
  "formatter": false,
  "model": "{env:OPENCODE_MODEL}",
  "agent": {
    "researcher": {
      "mode": "subagent",
      "model": "openai/gpt-5.3-codex",
      "variant": "xhigh",
      "prompt": "{file:./skills/researcher/SKILL.md}",
      "tools": {
        "*": true
      }
    }
  },
  "provider": {
    "openrouter": {
      "models": {
        "z-ai/glm-4.7": {
          "options": {
            "provider": {
              "order": [
                "z-ai"
              ],
              "allow_fallbacks": false
            }
          }
        },
        "openai/gpt-oss-20b": {
          "options": {
            "provider": {
              "order": [
                "groq"
              ],
              "allow_fallbacks": false
            }
          }
        },
        "openai/gpt-oss-120b": {
          "options": {
            "provider": {
              "order": [
                "cerebras"
              ],
              "allow_fallbacks": false
            }
          }
        },
        "minimax/minimax-m2.1": {
          "options": {
            "provider": {
              "order": [
                "minimax/lightning"
              ],
              "allow_fallbacks": false
            }
          }
        },
        "qwen/qwen3-coder-30b-a3b-instruct": {
          "options": {
            "provider": {
              "order": [
                "alibaba"
              ],
              "allow_fallbacks": false
            }
          }
        },
        "moonshotai/kimi-k2.5": {
          "options": {
            "provider": {
              "order": [
                "moonshotai"
              ],
              "allow_fallbacks": false
            }
          }
        }
      },
      "options": {
        "apiKey": "{env:OPENROUTER_API_KEY}"
      }
    },
    "llama.cpp": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "llama-server (local)",
      "options": {
        "baseURL": "http://127.0.0.1:8080/v1"
      },
      "models": {
        "qwen3-coder:a3b": {
          "name": "Qwen3-Coder: a3b-30b (local)",
          "limit": {
            "context": 128000,
            "output": 65536
          }
        },
        "gpt-oss-20b": {
          "name": "GPT-OSS-20B (local)",
          "limit": {
            "context": 131072,
            "output": 65536
          }
        },
        "gpt-oss-120b": {
          "name": "GPT-OSS-120B (local)",
          "limit": {
            "context": 262144,
            "output": 65536
          }
        },
        "glm-4.7-flash": {
          "name": "GLM-4.7 Flash (local)",
          "limit": {
            "context": 204800,
            "output": 131072
          }
        }
      }
    },
    "deepseek": {
      "options": {
        "apiKey": "{env:DEEPSEEK_API_KEY}"
      }
    },
    "google": {
      "name": "Google",
      "models": {
        "antigravity-gemini-3-pro": {
          "name": "Gemini 3 Pro (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65535
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingLevel": "low"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-gemini-3-flash": {
          "name": "Gemini 3 Flash (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "minimal": {
              "thinkingLevel": "minimal"
            },
            "low": {
              "thinkingLevel": "low"
            },
            "medium": {
              "thinkingLevel": "medium"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-claude-sonnet-4-5": {
          "name": "Claude Sonnet 4.5 (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        },
        "antigravity-claude-sonnet-4-5-thinking": {
          "name": "Claude Sonnet 4.5 Thinking (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          }
        },
        "antigravity-claude-opus-4-5-thinking": {
          "name": "Claude Opus 4.5 Thinking (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          }
        }
      }
    },
    "openai": {
      "options": {
        "reasoningEffort": "medium",
        "reasoningSummary": "auto",
        "textVerbosity": "medium",
        "include": [
          "reasoning.encrypted_content"
        ],
        "store": false
      },
      "models": {
        "gpt-5.2": {
          "options": {
            "reasoningEffort": "xhigh",
            "reasoningSummary": "detailed",
            "textVerbosity": "medium",
            "include": [
